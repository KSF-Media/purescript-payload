{
  "private": true,
  "scripts": {
    "clean": "rimraf output && rimraf .pulp-cache",
    "build": "pulp build -- --censor-lib --strict",
    "build:watch": "pulp --watch build -- --censor-lib --strict",
    "build:client:basic": "pulp --watch browserify --main Payload.Examples.Basic.Client --to output/examples/basicClient.js -- --censor-lib --strict",
    "test": "pulp --watch test --include examples --main Payload.Test.Main -- --censor-lib --strict --verbose-errors"
  },
  "devDependencies": {
    "bower": "1.8.8",
    "pulp": "13.0.0",
    "purescript": "0.13.0",
    "purescript-psa": "0.7.3",
    "rimraf": "2.5.4",
    "xhr2": "git:ssh://git@github.com:hoodunit/node-xhr2.git#6f65a6557706842b7284191f15ec9afaba270909"
  }
}
